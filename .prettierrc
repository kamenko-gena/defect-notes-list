{
  "endOfLine": "auto",
  "semi": true,
  "tabWidth": 4,
  "singleQuote": true,
  "bracketSpacing": true,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "html"
      }
    },
    {
      "files": "*.component.html",
      "options": {
        "parser": "angular"
      }
    }
  ]
}
